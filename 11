#include <stdio.h>

int main(void) {
	int x_p, y_p, x_s, y_s;
	int rez = 0;
	scanf("%d %d", &x_p, &y_p);
	scanf("%d %d", &x_s, &y_s);
	for(int i = 1; i<8; i++)
	{
		if(x_p == x_s + i && y_p == y_s + i) rez = 1;
		else if (x_p == x_s + i && y_p == y_s - i) rez = 1;
		else if (x_p == x_s - i && y_p == y_s - i) rez = 1;
		else if (x_p == x_s - i && y_p == y_s + i) rez = 1;
	}
	if (rez==1) printf("Осторожно, слон может срубить пешку!");
	else printf("Сейчас слон не может срубить пешку, но будьте начеку!");
	return 0;
}
