#include <stdio.h>

int main(void) {
	int x_p, y_p, x_s, y_s;
	int rez = 0;
	scanf("%d %d", &x_p, &y_p);
	scanf("%d %d", &x_s, &y_s);
	if (abs(x_p - y_p)==abs(x_s - y_s)) printf("Осторожно, слон может срубить пешку!");
	else printf("Сейчас слон не может срубить пешку, но будьте начеку!");
	return 0;
}
