#include <stdio.h>
 
int main(void) {
	int N;
    scanf("%d", &N);
    int n[N];
    for (int i = 0; i < N; i++) scanf("%d", &n[i]);
    for (int a = 0; a < N/2+1; a++)
    {
        int w = n[a];
        n[a] = n[N-1-a];
        n[N-1-a] = w;
    }
    for (int j = 0; j < N; j++)
    {
        printf("%d", n[j]);
    }
	return 0;
}
