#include <stdio.h>

int main(void) {
	int rez = 0;
	for (int i = 0; i < 1000000; i++)
	{
		int a = i/100000;
		int a1 = i%100000;
		int b = a1/10000;
		int b1 = i%10000;
		int c = b1/1000;
		int c1 = i%1000;
		int d = c1/100;
		int d1 = i%100;
		int e = d1/10;
		int f = i%10;
		if (a+b+c==d+e+f) rez++;
	}
	printf("%d", rez);
	return 0;
}
