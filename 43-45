#include <stdio.h>

int main(void) {
	int mas1[100] = {0};
	int mas2[100] = {0};
	int mas3[100] = {0};
	int a = 1;
	for (int i = 1; i<101; i++)
	{
		mas1[i-1] = i;
		mas2[i-1] = i*2;
		mas3[i-1] = i*2-1;
	}
	for (int i = 0; i<100; i++)
	{
		printf("%d	%d	%d \n", mas1[i], mas2[i], mas3[i]);
	}
	return 0;
}
