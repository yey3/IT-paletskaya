#include <stdio.h>

int main(void) {
	int N;
    scanf("%d", &N);
    int n[N];
    for (int i = 0; i < N; i++) scanf("%d", &n[i]);
    int a = 1;
    while (a != 0)
    {
        a = 0;
        for (int j = 0; j < N-1; j++)
        {
            if (n[j] > n[j+1])
            {
                int k = n[j];
                n[j] = n[j+1];
                n[j+1] = k;
                a++;
            }
        }
    }
    for (int i = 0; i < N; i++) printf("%d", n[i]);
	return 0;
}
