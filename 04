#include <stdio.h>
#include <math.h>

int main(void) {
	int n;
	scanf("%d", &n);
	int a = 0;
	for(int i = 2; i<sqrt(n)+1; i++)
	{
		if (i == sqrt(n)) printf("Число %d делится на %d. \n", n, i);
		else if(n%i == 0)
		{
			printf("Число %d делится на %d и %d. \n", n, i, n/i);
			a++;
		}
	}
	if(a==0)
	{
		printf("Число %d простое", n);
	}
	return 0;
}
