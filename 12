#include <stdio.h>

int main(void) {
	int x_p, y_p, x_l, y_l;
	int rez = 0;
	scanf("%d %d", &x_p, &y_p);
	scanf("%d %d", &x_l, &y_l);
	for(int i = 1; i<8; i++)
	{
		if(x_p == x_l + i && y_p == y_l) rez = 1;
		else if (x_p == x_l && y_p == y_l + i) rez = 1;
		else if (x_p == x_l && y_p == y_l - i) rez = 1;
		else if (x_p == x_l - i && y_p == y_l) rez = 1;
	}
	if (rez==1) printf("Осторожно, ладья может срубить пешку!");
	else printf("Сейчас ладья не может срубить пешку, но будьте начеку!");
	return 0;
}
