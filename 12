#include <stdio.h>

int main(void) {
	int x_p, y_p, x_l, y_l;
	int rez = 0;
	scanf("%d %d", &x_p, &y_p);
	scanf("%d %d", &x_l, &y_l);
	int x = abs(x_p - x_l);
	int y = abs(y_p - y_l);
	if (y==0 || x==0) printf("Осторожно, ладья может срубить пешку!");
	else printf("Сейчас ладья не может срубить пешку, но будьте начеку!");
	return 0;
}
