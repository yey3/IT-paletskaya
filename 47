#include <stdio.h>

int main(void) {
	int N;
	scanf("%d", &N);
	double mas[N];
	for (int i = 0; i < N; i++)
	{
		scanf("%lf", &mas[i]);
	}
	double min = mas[0];
	double max = mas[0];
	for (int i = 1; i < N; i++)
	{
		if (mas[i] < min) min = mas[i];
		if (mas[i] > max) max = mas[i];
	}
	printf("%lf %lf", min, max);
	return 0;
}
